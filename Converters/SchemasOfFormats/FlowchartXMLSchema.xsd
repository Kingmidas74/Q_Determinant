<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="XMLSchema"
    targetNamespace="http://tempuri.org/XMLSchema.xsd"
    elementFormDefault="qualified"
    xmlns="http://tempuri.org/XMLSchema.xsd"
    xmlns:mstns="http://tempuri.org/XMLSchema.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
>
  <xs:element name="FlowChart">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Conditions">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Block" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute type="xs:long" name="Id"/>
							<xs:attribute type="xs:long" name="TruePath"/>
							<xs:attribute type="xs:long" name="FalsePath"/>
						</xs:extension>
					</xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Process">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Block" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute type="xs:long" name="Id"/>
							<xs:attribute type="xs:long" name="NextElement"/>
						</xs:extension>
					</xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Output">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Block" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute type="xs:long" name="Id"/>
							<xs:attribute type="xs:long" name="NextElement"/>
						</xs:extension>
					</xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
		<xs:element name="Input">
          <xs:complexType>
				<xs:simpleContent>
					<xs:extension base="xs:string">
						<xs:attribute type="xs:long" name="Id"/>
						<xs:attribute type="xs:long" name="NextElement"/>
					</xs:extension>
				</xs:simpleContent>
			</xs:complexType>
        </xs:element>
        <xs:element name="Start" maxOccurs="1" minOccurs="1">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute type="xs:long" name="Id"/>
				<xs:attribute type="xs:long" name="NextElement"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
        <xs:element name="End" maxOccurs="1" minOccurs="1">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute type="xs:long" name="Id"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
